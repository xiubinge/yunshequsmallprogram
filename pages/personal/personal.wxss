/* pages/personal/personal.wxss */
.wd_header{
    width:100%;
    height:320rpx;
    background: url(../../images/wode/07.png) no-repeat;
    background-size:cover;
    padding:0 30rpx;
    box-sizing:border-box;
}
.edit{
    width:100%;
    height: 48rpx;
    text-align: right;
    padding-top: 10rpx;
    box-sizing:border-box;
}
.edit_icon{
    display: inline-block;
    width:38rpx;
    height:36rpx;
}
.edit_icon image{
    display: inline-block;
    width:38rpx;
    height: 36rpx;
}
.wd_img{
    width:100%;
    height: auto;
    text-align: center;
}
.wd_img_in{
    display: inline-block;
    width:136rpx;
    height: 136rpx;
    border-radius: 50%;
    border:2rpx solid #fff;
    overflow: hidden;
}
.wd_img_in image{
    width:136rpx;
    height:136rpx;
}
.img_txt{
    font-size: 32rpx;
    color:#fff;
    text-align: center;
    margin-top: 20rpx;
}
.wd_list{
    width:100%;
    height: auto;
    margin-top: 30rpx;
    background-color: #fff;
}
.list{
    width:100%;
    height: auto;
    align-items: center;
    -webkit-box-align: center;
    -webkit-align-items: center;
    -ms-flex-align: center;
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    height: 90rpx;
    padding:0 30rpx;
    box-sizing:border-box;
    position: relative;
    overflow: hidden;
}
.list:after{
    content: "";
    position: absolute;
    bottom:0;
    left: 90rpx;
    width:100%;
    height: 1rpx;
    border-bottom:1rpx solid #eee;
}
.list:last-child:after{
    border:none;
}
.list_left{
    webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    -ms-flex: 1;
    flex: 1;
}
.list_icon{
    width:60rpx;
}
.icon01{
    display: inline-block;
    width:32rpx;
    height: 33rpx;
    vertical-align: -3rpx;
}
.list_left{
    font-size:32rpx;
    color:#333;
}
.list_arrow{
    display: inline-block;
    display: inline-block;
    height: 10px;
    width: 10px;
    border-width: 1px 1px 0 0;
    border-color: #C8C8CD;
    border-style: solid;
    -webkit-transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
    transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
}
.list_num{
    font-size: 26rpx;
    color:#fe7a7a;
    margin:0 20rpx;
}
.icon02{
    width:35rpx;
    height: 32rpx;
}
.icon03{
    width:32rpx;
    height: 33rpx;
}
.icon04{
    width:33rpx;
    height: 38rpx;
    vertical-align: -2rpx;
}
.icon05{
    width:34rpx;
    height: 34rpx;
    vertical-align: 2rpx;
}